alias pyvenv="python -m venv .env && source .env/bin/activate"
alias pysetup="mkdir my_module && touch setup.py && mkdir tests && touch tests/test_app.py && touch my_module/__init__.py && pip install -e ."
alias python=python3

alias ?="searchweb"
alias ordnet="searchweb -page=ordnet"
alias korpus="searchweb -page=korpus"
alias tysk="searchweb -page=tysk"
alias youtube="searchweb -page=youtube"
alias github="searchweb -page=github"

